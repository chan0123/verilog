#! /usr/local/bin/vvp
:ivl_version "10.1 (stable)" "(v10_1-71-g8ab100c1)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
:vpi_module "v2009";
S_0x7fce0cd02760 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x7fce0cd179d0_0 .var "clk", 0 0;
v0x7fce0cd17a70_0 .var "reset", 0 0;
v0x7fce0cd17b20_0 .net "value", 7 0, v0x7fce0cd178a0_0;  1 drivers
S_0x7fce0cd079e0 .scope module, "c1" "counter" 2 18, 3 1 0, S_0x7fce0cd02760;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "reset"
P_0x7fce0cd00870 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x7fce0cd028c0_0 .net "clk", 0 0, v0x7fce0cd179d0_0;  1 drivers
v0x7fce0cd178a0_0 .var "out", 7 0;
v0x7fce0cd17940_0 .net "reset", 0 0, v0x7fce0cd17a70_0;  1 drivers
E_0x7fce0cd00a90 .event edge, v0x7fce0cd17940_0;
E_0x7fce0cd017c0 .event posedge, v0x7fce0cd028c0_0;
    .scope S_0x7fce0cd079e0;
T_0 ;
    %wait E_0x7fce0cd017c0;
    %load/vec4 v0x7fce0cd178a0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x7fce0cd178a0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x7fce0cd079e0;
T_1 ;
    %wait E_0x7fce0cd00a90;
    %load/vec4 v0x7fce0cd17940_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %cassign/vec4 v0x7fce0cd178a0_0;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x7fce0cd178a0_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x7fce0cd02760;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fce0cd17a70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fce0cd179d0_0, 0, 1;
    %end;
    .thread T_2, $init;
    .scope S_0x7fce0cd02760;
T_3 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fce0cd17a70_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fce0cd17a70_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x7fce0cd17a70_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x7fce0cd17a70_0, 0, 1;
    %delay 100, 0;
    %vpi_call/w 2 10 "$stop" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x7fce0cd02760;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x7fce0cd179d0_0;
    %nor/r;
    %store/vec4 v0x7fce0cd179d0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x7fce0cd02760;
T_5 ;
    %vpi_call/w 2 22 "$dumpfile", "test.vcd" {0 0 0};
    %vpi_call/w 2 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x7fce0cd02760 {0 0 0};
    %vpi_call/w 2 24 "$monitor", "At time %t, value = %h (%0d)", $time, v0x7fce0cd17b20_0, v0x7fce0cd17b20_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
